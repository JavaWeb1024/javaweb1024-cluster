			</if>
			<if test="adminVo.loginIp != null">
				login_ip,
			</if>
			<if test="adminVo.name != null">
				name,
			</if>
			<if test="adminVo.password != null">
				password,
			</if>
			<if test="adminVo.username != null">
				username,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="adminVo.createDate != null">
